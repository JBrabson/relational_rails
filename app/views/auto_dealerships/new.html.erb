<h5><a href="/automobiles"><%= "All Automobiles" %></a></h5><br>
<h5><a href="/auto_dealerships"><%= "All Dealerships" %></a></h5><br>
<h3>Create a New Auto Dealership</h3>
<%= form_with url: "/auto_dealerships", method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <br>
  <%= form.label :preowned_sales %>
  <%= form.text_field :preowned_sales %>
  <br>
  <%= form.label :foreign_inventory %>
  <%= form.text_field :foreign_inventory %>
  <br>
  <%= form.label :city %>
  <%= form.text_field :city %>
  <br>
  <%= form.label :state %>
  <%= form.text_field :state %>
  <%= form.submit "Create Auto Dealership" %>
<% end %>
