<%= link_to "All Music Stores |", "/music_stores" %>
<%= link_to "All Instruments", "/instruments" %>
<%= form_with url: "/music_stores/#{@music_store.id}/instruments", method: :post, local: true do |form| %>
  <%= form.label :kind %>
  <%= form.text_field :kind %>
  <%= form.label :rent_to_own %>
  <%= form.text_field :rent_to_own %>
  <%= form.label :cost %>
  <%= form.text_field :cost %>
  <%= form.submit "Create Instrument" %>
<% end %>