<p id="notice"><%= notice %></p>

<h1>Automobiles</h1>

<table>
  <thead>
    <tr>
      <th>Preowned</th>
      <th>Year</th>
      <th>Make</th>
      <th>Model</th>
      <th>Color</th>
      <th>Automatic</th>
      <th>Engine</th>
      <th>Horsepower</th>
      <th>Cylinders</th>
      <th>Drive train</th>
      <th>Price</th>
      <th>Seating capacity</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @automobiles.each do |automobile| %>
      <tr>
        <td><%= automobile.preowned %></td>
        <td><%= automobile.year %></td>
        <td><%= automobile.make %></td>
        <td><%= automobile.model %></td>
        <td><%= automobile.color %></td>
        <td><%= automobile.automatic %></td>
        <td><%= automobile.engine %></td>
        <td><%= automobile.horsepower %></td>
        <td><%= automobile.cylinders %></td>
        <td><%= automobile.drive_train %></td>
        <td><%= automobile.price %></td>
        <td><%= automobile.seating_capacity %></td>
        <td><%= link_to 'Show', automobile %></td>
        <td><%= link_to 'Edit', edit_automobile_path(automobile) %></td>
        <td><%= link_to 'Destroy', automobile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Automobile', new_automobile_path %>
